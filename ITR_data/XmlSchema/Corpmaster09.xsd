						<xs:documentation>Enter Date of filing of Original return</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:date">
							<xs:minInclusive value="2009-04-01"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ResidentialStatus" default="RES" id="FilStatSec6">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="3"/>
							<xs:enumeration value="RES"/>
							<xs:enumeration value="NRI"/>
							<xs:enumeration value="NOR"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="NRI_PE" default="N">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="1"/>
							<xs:enumeration value="N"/>
							<xs:enumeration value="Y"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="AsseseeRepFlg" default="N" id="AseRepSec1">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="1"/>
							<xs:enumeration value="Y"/>
							<xs:enumeration value="N"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="AssesseeRep" id="FilStatSec7" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
