						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Phone" id="AdrSec8" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="STDcode" default="0" id="AdrSec9">
								<xs:simpleType>
									<xs:restriction base="xs:unsignedLong">
										<xs:totalDigits value="5"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="PhoneNo" default="0" id="AdrSec10">
								<xs:simpleType>
									<xs:restriction base="xs:unsignedLong">
										<xs:totalDigits value="10"/>
