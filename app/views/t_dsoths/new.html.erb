<h1>New t_dsoth</h1>
<% form_for(@t_dsoth) do |f| %>

<%= f.error_messages %>
<table id="cart">
    <tbody id="line_items">
    <tr>
        <td style="width: 20%">
            <%= f.label :tan %>
        </td>
        <br/>
        <td>
            <%= f.text_field :tan %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :utn %>
        </td>
        
        <td>
            <%= f.text_field :utn %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :employerOrDeductorOrCollecterName %>
        </td>
        
        <td>
            <%= f.text_field :employerOrDeductorOrCollecterName %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :addrDetail %>
        </td>
        
        <td>
            <%= f.text_field :addrDetail %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :cityOrTownOrDistrict %>
        </td>
        
        <td>
            <%= f.text_field :cityOrTownOrDistrict %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :pinCode %>
        </td>
        
        <td>
            <%= f.text_field :pinCode %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :stateCode %>
        </td>
        
        <td>
            <%= f.select :stateCode,
            Form::INDIAN_STATES,
            :prompt => "Select State" %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :amtPaid %>
        </td>
        
        <td>
            <%= f.text_field :amtPaid %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :datePayCred %>
        </td>
        
        <td>
            <%= f.date_select :datePayCred ,
            :order => [:day, :month, :year] %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :totTDSOnAmtPaid %>
        </td>
        
        <td>
            <%= f.text_field :totTDSOnAmtPaid %>
        </td>
    </tr>
    <tr>
        <td>
            <%= f.label :claimOutOfTotTDSOnAmtPaid %>
        </td>
        
        <td>
            <%= f.text_field :claimOutOfTotTDSOnAmtPaid %>
        </td>
    </tr>
    <!--
    <tr>
    <td> <%= f.label :TaxPaidAndRefund_id %> </td><br />
    <td> <%= f.text_field :tax_paid_and_refund_id %> </td>
    </tr>
    -->
    <tr>
        <td>
            <%= f.submit "Create" %>
        </td>
    </tr>
    <% end %>
    </td>
    <td>
        <%= link_to 'Back', t_dsoths_path %>
    </td>
</td>
</tbody>
</table>